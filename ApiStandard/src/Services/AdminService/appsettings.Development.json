{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Components": {
    "Cache": "Memory",
    "Database": "PostgreSQL",
    "UseCors": true,
    "AuthType": "Jwt",
    "MQType": "None",
    "UseSMS": false,
    "UseSmtp": false,
    "UseAWSS3": false,
    "UseOpenAPI": true
  },
  "Authentication": {
    "Jwt": {
      "ValidAudiences": "AdminService",
      "ValidIssuer": "https://localhost:7070",
      "Sign": "MyProjectName.MakeDotnetGreatAgain.Perigon",
      "ExpiredSecond": 7200
    },
    "OAuth": {
      "Authority": "https://localhost:7070",
      "Audiences": [ "AdminService" ],
      "ClientId": "AdminService",
      "ValidateAudience": true,
      "RequireHttpsMetadata": false
    }
  },
  "LoginSecurityPolicy": {
    "PasswordLevel": 0,
    "IsNeedVerifyCode": false,
    "PasswordExpired": 90,
    "LoginRetry": 5,
    "SessionLevel": 0
  }
}
