{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "Components": {
    "Cache": "Memory",
    "Database": "PostgreSQL",
    "UseCors": true,
    "AuthType": "Jwt",
    "MQType": "None",
    "UseSMS": false,
    "UseSmtp": false,
    "UseAWSS3": false,
    "UseOpenAPI": true
  },
  "Authentication": {
    "Jwt": {
      "ValidAudiences": "ApiService",
      "ValidIssuer": "https://localhost:7070",
      "Sign": "MyProjectName.FightForFreedomForever.Justice",
      "ExpiredSecond": 7200
    },
    "OAuth": {
      "Authority": "https://localhost:7070",
      "Audiences": [ "AdminService" ],
      "ClientId": "ApiClient",
      "ValidateAudience": true,
      "RequireHttpsMetadata": false
    }
  },
  "LoginSecurityPolicy": {
    "PasswordLevel": 0,
    "IsNeedVerifyCode": false,
    "PasswordExpired": 90,
    "LoginRetry": 5,
    "SessionLevel": 0
  },

  "ConnectionStrings": {
    "CommandDb": "Server=127.0.0.1;Port=15432;Database=MyProjectName;User Id=postgres;Password=MyProjectName_DevSecret;",
    "QueryDb": "Server=127.0.0.1;Port=15432;Database=MyProjectName;User Id=postgres;Password=MyProjectName_DevSecret;",
    "Cache": "localhost:16379,password=MyProjectName_DevSecret"
  }
}